{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
        "semi": [2, "never"],
        "indent": [2, 4],
        "no-unused-vars": 0,
        "no-console": 0,
        "arrow-parens": [2, "as-needed"],
        "object-curly-newline": 0,
        "comma-dangle": [2, "never"],
        "prefer-destructuring": 0,
        "no-param-reassign": [2, { "props": false }]
    },
    "globals": {
        "__dirname": true
    }
}
